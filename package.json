{
  "name": "upswyng",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/google-map-react": "^0.23.11",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "firebase": "^5.8.4",
    "google-map-react": "^1.1.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "typescript": "^3.3.4000"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "datapipe": "npm run strapped && npm run filter",
    "filter": "node data_pipeline/filter.js",
    "strapped": "npm run strapped-charity && npm run strapped-crisisline",
    "strapped-charity": "curl -o data_pipeline/strapped/charity.json https://strapped.firebaseio.com/charity.json",
    "strapped-crisisline": "curl -o data_pipeline/strapped/crisisline.json https://strapped.firebaseio.com/crisisline.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "1.16.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
